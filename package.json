{
  "name": "applaus-react",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://yorickreum.github.io/react-applaus-o-meter/",
  "main": "index.js",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-preset-react-app": "^9.0.0",
    "bootstrap": "^4.3.1",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-bootstrap-table": "^4.3.1",
    "react-dom": "^16.8.6",
    "react-edit": "^6.4.1",
    "react-mic": "^12.4.1",
    "react-motions": "^0.3.0",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "react-svgmt": "^1.1.8",
    "redux": "^4.0.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/View/styles --include-path ./node_modules src/View/styles/ -o src/View/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/View/styles --include-path ./node_modules src/View/styles/ -o src/View/styles --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "redux-devtools": "^3.5.0"
  }
}

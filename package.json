{
  "name": "applaus-react",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://slammenimschloss.de/fishtival/",
  "main": "index.js",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.2",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-preset-react-app": "^3.1.2",
    "bootstrap": "^4.1.3",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.5.1",
    "react-bootstrap-table": "^4.3.1",
    "react-dom": "^16.5.1",
    "react-edit": "^6.4.1",
    "react-mic": "^12.2.2",
    "react-motions": "^0.3.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "react-svgmt": "^1.1.6"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/View/styles --include-path ./node_modules src/View/styles/ -o src/View/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/View/styles --include-path ./node_modules src/View/styles/ -o src/View/styles --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
